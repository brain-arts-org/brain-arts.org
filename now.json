{
  "version": 2,
  "alias": ["brain-arts.org", "www.brain-arts.org"],
  "name": "brain-arts-org",
  "builds": [{ "src": "next.config.js", "use": "@now/next" }],
  "routes": [
    {
      "src": "/(.*)",
      "status": 301,
      "headers": { "Location": "https://brain-arts.org" }
    }
  ],
  "env": {
    "TAKESHAPE_PROJECT": "@takeshape-project",
    "TAKESHAPE_TOKEN": "@takeshape-token"
  },
  "build": {
    "env": {
      "TAKESHAPE_PROJECT": "@takeshape-project",
      "TAKESHAPE_TOKEN": "@takeshape-token"
    }
  }
}
